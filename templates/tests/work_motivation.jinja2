{% extends "default.jinja2" %}
{% block head %}
{% endblock %}
{% block content %}
	<main class="container">
	{% for question in questions %}
		<div class="row">
			<div class="col-lg-1 col-2">{{ question.id }}</div>
			<div class="col-lg-6 col-10"><p>...{{ question.title }}</p></div>
			<div class="col-lg-1 col-2"><button class="btn">1</button></div>
			<div class="col-lg-1 col-2"><button class="btn">2</button></div>
			<div class="col-lg-1 col-2"><button class="btn">3</button></div>
			<div class="col-lg-1 col-2"><button class="btn">4</button></div>
			<div class="col-lg-1 col-2"><button class="btn">5</button></div>
		</div>
	{% endfor %}
	</main>
{% endblock %}
